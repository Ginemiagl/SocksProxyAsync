{
    "name": "postuf/socks-proxy-async",
    "type": "library",
    "description": "Lightweight asynchronous socks proxy for PHP",
    "homepage": "https://github.com/Postuf/SocksProxyAsync",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SocksProxyAsync\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/wkejqb/php-dns-client"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "require": {
        "ext-sockets": "*",
        "metaregistrar/php-dns-client": "dev-async"
    }
}
